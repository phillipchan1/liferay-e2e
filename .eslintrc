{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"extends": ["eslint:recommended", "prettier"],
	"plugins": ["prettier"],
	"env": {
		"es6": true,
		"browser": true
	},
	"globals": {
		"__dirname": true,
		"cy": true,
		"document": true,
		"describe": true,
		"it": true,
		"module": true,
		"process": true,
		"window": true
	},
	"rules": {
		"prettier/prettier": "error",
		"arrow-body-style": 0,
		"default-case": 0,
		"global-require": 0,
		"no-confusing-arrow": 0,
		"no-console": 0,
		"no-constant-condition": 0,
		"no-shadow": 0,
		"padded-blocks": 0
	}
}
